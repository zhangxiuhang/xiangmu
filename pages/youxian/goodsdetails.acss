/*******bottom btn******/
.bottom {
    width: 100%;
    bottom:0px;
    position:fixed;
    background-color: #fff;
    border-top: 1px solid #e5e5e5;
}
.buybutton-style1 {
    background: rgba(250,250,249,1);
    bottom: 0rem;
    height: 45px;
    line-height: 45px;
    width: 100%;
    display: block;
}
.buybutton-style1 .ulli{
    float:left;
    align-items: center;
    justify-content: center;
}

.buybutton-style1 .buybutton-shoucang{
    display: -webkit-flex;
    align-items: center;
}

/* .buybutton-style1 .ulli:nth-child(1) {
    width: 29%;
    box-sizing: border-box;
    border-right: 1px solid #eaedf2;
} */

.buybutton-style1 .ulli:nth-child(1) {
    text-align: center;
    width: 15%;
    box-sizing: border-box;
    border-right: 1px solid #eaedf2;
}
.buybutton-style1 .ulli:nth-child(2) {
    text-align: center;
    width: 15%;
    box-sizing: border-box;
    border-right: 1px solid #eaedf2;
}
.buybutton-style1 .ulli:nth-child(3) {
    text-align: center;
    width: 15%;
    box-sizing: border-box;
    border-right: 1px solid #eaedf2;
}

.buy_icon{
    position: absolute;
    z-index: 2;
    background: #f60;
    color: #fff;
    width: 12px;
    height: 12px;
    line-height: 12px;
    overflow: hidden;
    top: 5px;
    right:56%;
    padding: 3px;
    border-radius: 50%;
    font-size: 12px;
}

.buybutton-style1 .buybutton-shoucang image {
    width: 20px;
    height: 20px;
    margin:13px 0px 0px;
}

.buybutton-style1 .ulli:nth-child(4) {
    width: 28%;
    color: #fff;
    background: #ff9600;
}

.buybutton-style1 .ulli:nth-child(5) {
    background: #ef4746;
    color: #fff;
    width: 27%;
}

